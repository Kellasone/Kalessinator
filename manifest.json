{
  "name": "Kalessinator",
  "description": "Small addon to kalessinate the FOE game.",
  "version": "0.0.1",
  "manifest_version":3,
  "background": {
    "service_worker":"./klsnt-init/inject.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [{
    "resources": [
      "/klsnt-common/style/images/cursor/*.png",
      "/klsnt-opener/images/*.png",
      "/klsnt-window/images/*.png"
    ],
    "matches": ["<all_urls>"]
  }]
}